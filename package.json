{
    "name": "car-dealership-landing",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "NODE_OPTIONS='--max-old-space-size=2048 --expose-gc' next dev --turbopack",
        "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
        "start": "NODE_OPTIONS='--max-old-space-size=2048' next start",
        "lint": "next lint",
        "lint:fix": "next lint --fix",
        "type-check": "tsc --noEmit",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "dotenv -c production -- drizzle-kit migrate",
        "db:studio": "drizzle-kit studio",
        "db:setup-fresh": "tsx scripts/setup-fresh-db.ts",
        "db:reset": "rm -rf drizzle/*.sql drizzle/meta/*.json && echo '{ \"version\": \"7\", \"dialect\": \"postgresql\", \"entries\": [] }' > drizzle/meta/_journal.json",
        "health-check": "curl -f http://localhost:3000/api/health || exit 1",
        "production-check": "npm run type-check && npm run lint && npm run build",
        "analyze": "ANALYZE=true npm run build",
        "test-fonts": "tsx scripts/test-fonts.ts"
    },
